ENTRY(_start)

SECTIONS {
        .text 0x111C0000 : {
            _start = .;
            *(.text._start)
            *(.text.*)
        }
		
        .bss : {
            *(.bss*)
        }

        .data : {
            *(.rodata*)
            *(.data*)
        }
    /* Discard the GOT - we're not using any relocations that need a GOT and the corresponding relocation code */
    /*.got : {}
    /DISCARD/ : { *(.got); } */
        _end = .;
}
